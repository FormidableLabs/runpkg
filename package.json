{
  "name": "runpkg",
  "version": "1.0.0",
  "description": "The online javascript package explorer",
  "main": "src/index.js",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "prettier": "prettier --config .prettierrc --write \"./**/*.{css,html,json,md,yml}\"",
    "lint": "eslint \"./**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FormidableLabs/runpkg.git"
  },
  "author": "Luke Jackson <@lukejacksonn>",
  "contributors": [
    "Alex Saunders <@alex-saunders>",
    "Kara Stubbs <@kiraarghy>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/runpkg/issues"
  },
  "homepage": "https://runpkg.com",
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0",
    "servor": "^3.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-pwa": "^0.12.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{css,html,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "prettier",
      "prettier/react"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 10,
      "sourceType": "module"
    },
    "rules": {
      "no-useless-escape": "off",
      "no-console": "off"
    }
  },
  "dependencies": {
    "algoliasearch": "4.1.0",
    "csz": "1.1.1",
    "htm": "2.2.1",
    "marked": "4.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.5",
    "workbox-precaching": "^6.5.3"
  }
}
